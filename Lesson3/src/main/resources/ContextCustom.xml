<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p = "http://www.springframework.org/schema/p"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>

    <bean id="userCustom" class="Lesson3.Annotation.UserCustom" p:name="Egor"/>
    <bean id="userInject" class="Lesson3.Annotation.UserInject" p:name="Fed"/>
    <bean id="userProvider" class="Lesson3.Annotation.UserProvider" p:name="Dima"/>
    <bean id="userNamed" class="Lesson3.Annotation.UserNamed" p:name="Fav"/>
    <bean id="userInjectAnnotation" class="Lesson3.Annotation.UserInjectAnnotation" p:name="Gena"/>
    <bean id="userValue" class="Lesson3.Annotation.UserValue" p:name="Lena"/>
    <bean id="userResource" class="Lesson3.Annotation.UserResource" p:name="Ken"/>
    <bean id="info" class="Lesson3.Annotation.Info" p:balance="100"/>
    <bean id="infoAboutUser" class="Lesson3.Annotation.InfoAboutUser" p:string="str"/>
</beans>